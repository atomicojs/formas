{
    "name": "@formas/input-list",
    "version": "1.0.0",
    "type": "module",
    "publishConfig": {
        "access": "public"
    },
    "peerDependencies": {
        "atomico": "*"
    },
    "scripts": {
        "build:types": "tsc",
        "build:compile": "library src/**/*",
        "build:exports": "exports lib/**/* types/**/* --wrappers",
        "dev:exports": "exports src/**/* --watch",
        "build": "npm run build:types && npm run build:compile && npm run build:exports",
        "prepublishOnly": "npm run build"
    },
    "types": "src/index.tsx",
    "exports": {
        "./element": "./src/element.tsx",
        ".": "./src/index.tsx",
        "./elements": "./src/elements.ts",
        "./InputList": "./src/InputList.tsx",
        "./InputListOption": "./src/InputListOption.tsx",
        "./InputListBadge": "./src/InputListBadge.tsx",
        "./utils": "./src/utils.ts"
    },
    "typesVersions": {
        "*": {
            "element": [
                "src/element.tsx"
            ],
            "elements": [
                "src/elements.ts"
            ],
            "InputList": [
                "src/InputList.tsx"
            ],
            "InputListOption": [
                "src/InputListOption.tsx"
            ],
            "InputListBadge": [
                "src/InputListBadge.tsx"
            ],
            "utils": [
                "src/utils.ts"
            ]
        }
    }
}