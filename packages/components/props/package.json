{
    "name": "@formas/props",
    "version": "1.0.1",
    "type": "module",
    "publishConfig": {
        "access": "public"
    },
    "peerDependencies": {
        "atomico": "*"
    },
    "scripts": {
        "build:types": "tsc",
        "build:compile": "library \"src/**/*\" --all-external",
        "build:exports": "exports \"lib/**/*\" \"types/**/*\" --wrappers",
        "dev:exports": "exports \"src/**/*\" --watch"
    },
    "types": "types/index.d.ts",
    "exports": {
        ".": {
            "types": "./types/index.d.ts",
            "default": "./lib/index.js"
        },
        "./README.md": {
            "default": "./types/README.md"
        }
    },
    "typesVersions": {
        "*": {}
    }
}
